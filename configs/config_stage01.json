{
    "baseUrl": "https://tv.nrk.no",
    "mutateMetadata": false,
    "contentContainers": [
        {
            "type": "firstItem",
            "index": 0,            
            "timeout": 3,
            "locateBy": "CSS_SELECTOR",
            "locateByValue": "ol.list-style-none",
            "contentItem": {
                "type": "listUniqKeys",
                "locateBy": "CSS_SELECTOR",
                "locateByValue": "a.text-style-subhead1",
                "selectValues": [
                    {
                        "selectTarget": "attribute",
                        "selectValue": "href",
                        "keyName": "url"
                    },
                    {
                        "selectTarget": "text",
                        "keyName": "title"
                    }
                ]
            }
        }
    ]
}
